{
    "name": "fh3095/forms",
    "type": "phpbb-extension",
    "description": "Forms for phpbb",
    "homepage": "https://github.com/FH3095/phpbb-forms",
    "version": "1.0.0",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "FH3095",
