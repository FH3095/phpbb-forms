	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
    "extra": {
        "display-name": "phpbb Forms",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0,<3.2.*@dev",
